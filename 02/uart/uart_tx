S_0x55e9cf76f4a0 .scope module, "uart_tx" "uart_tx" 2 1;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 8 "data_in";
    .port_info 3 /OUTPUT 1 "tx_en";
    .port_info 4 /OUTPUT 1 "tx_data";
o0x7efc4199f018 .functor BUFZ 1, C4<z>; HiZ drive
v0x55e9cf74a7f0_0 .net "clk", 0 0, o0x7efc4199f018;  0 drivers
o0x7efc4199f048 .functor BUFZ 8, C4<zzzzzzzz>; HiZ drive
v0x55e9cf74ac00_0 .net "data_in", 7 0, o0x7efc4199f048;  0 drivers
o0x7efc4199f078 .functor BUFZ 1, C4<z>; HiZ drive
v0x55e9cf793b50_0 .net "rst", 0 0, o0x7efc4199f078;  0 drivers
v0x55e9cf793c20_0 .var "state", 1 0;
v0x55e9cf793d00_0 .var "tx_data", 0 0;
v0x55e9cf793e10_0 .var "tx_en", 0 0;
E_0x55e9cf781f50 .event posedge, v0x55e9cf74a7f0_0;
    .scope S_0x55e9cf76f4a0;
T_0 ;
    %wait E_0x55e9cf781f50;
    %load/vec4 v0x55e9cf793b50_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x55e9cf793c20_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55e9cf793e10_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55e9cf793d00_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55e9cf793c20_0;
    %pad/u 4;
    %dup/vec4;
    %pushi/vec4 0, 0, 4;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 4;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 8, 0, 4;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %jmp T_0.5;
T_0.2 ;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55e9cf793e10_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55e9cf793d00_0, 0;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x55e9cf793c20_0, 0;
    %jmp T_0.5;
T_0.3 ;
    %load/vec4 v0x55e9cf74ac00_0;
    %pushi/vec4 7, 0, 32;
    %load/vec4 v0x55e9cf793c20_0;
    %pad/u 32;
    %sub;
    %part/u 1;
    %assign/vec4 v0x55e9cf793d00_0, 0;
    %load/vec4 v0x55e9cf793c20_0;
    %addi 1, 0, 2;
    %assign/vec4 v0x55e9cf793c20_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55e9cf793e10_0, 0;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x55e9cf793c20_0, 0;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
